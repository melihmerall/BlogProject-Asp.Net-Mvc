@model  IEnumerable<Entity.Concrete.Blog>
@foreach (var value in Model)
{
    <div class="section-row">
        <div class="section-title">
            <h3 class="title">Author <a href="author.html">@value.Author.AuthorName</a></h3>
        </div>
        <div class="author media">
            <div class="media-left">
                <a href="author.html">
                    <img class="author-img media-object" src="@value.Author.AuthorImage" alt="">
                </a>
            </div>
            <div class="media-body">
                <p>@value.Author.AuthorDetail</p>
                <ul class="author-social">
                    <li><a href="@value.Author.AuthorLinkedin" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                    <li><a href="@value.Author.AuthorTwitter" target="_blank"><i class="fa fa-twitter"></i></a></li>
                    <li><a href="@value.Author.AuthorInstagram" target="_blank"><i class="fa fa-instagram"></i></a></li>
                </ul>
            </div>
        </div>
    </div>
}